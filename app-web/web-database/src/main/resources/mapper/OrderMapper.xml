        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.xds.wtuapp.web.database.mapper.OrderMapper">

    <resultMap id="BaseResultMap" type="pers.xds.wtuapp.web.database.bean.Order">
